{
  "name": "with-staged",
  "description": "run a command (like linting) on git staged files",
  "version": "1.0.2",
  "author": "RenÃ©e Kooi <renee@kooi.me>",
  "bugs": {
    "url": "https://github.com/goto-bus-stop/with-staged/issues"
  },
  "dependencies": {
    "micromatch": "^2.3.11",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "husky": "^2.2.0",
    "rimraf": "^2.6.3",
    "semver": "^6.0.0",
    "simple-concat": "^1.0.0",
    "standard": "^14.3.3",
    "tap-min": "^2.0.0",
    "tap-spec": "^5.0.0",
    "tape": "^5.0.1",
    "unique-temp-dir": "^1.0.0"
  },
  "homepage": "https://github.com/goto-bus-stop/with-staged",
  "keywords": [
    "check",
    "command",
    "eslint",
    "format",
    "git",
    "lint",
    "linter",
    "staged",
    "standard"
  ],
  "license": "Apache-2.0",
  "main": "index.js",
  "bin": "bin.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/goto-bus-stop/with-staged.git"
  },
  "scripts": {
    "lint-staged": "./bin.js '*.js' -- standard",
    "tap-min": "node test | tap-min",
    "test": "standard && node test | tap-spec"
  },
  "husky": {
    "hooks": {
      "precommit": "npm run lint-staged && npm run tap-min"
    }
  }
}
